{
    "name": "bain-wizard-language-support",
    "displayName": "bain-wizard-language-support",
    "description": "Add support for BAIN Wizard scripting language.",
    "version": "0.0.1",
    "publisher": "Holt59",
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "bainwizard",
                "aliases": [
                    "BAIN Wizard"
                ],
                "extensions": [
                    "wizard.txt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "bainwizard",
                "scopeName": "source.wizard",
                "path": "./syntaxes/wizard.tmLanguage.json"
            }
        ]
    }
}
