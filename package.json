{
    "name": "bain-wizard-language-support",
    "displayName": "BAIN Wizard Language Support",
    "description": "Add support for BAIN Wizard scripting language.",
    "version": "0.0.2",
    "publisher": "Holt59",
    "repository": {
        "type": "git",
        "url": "https://github.com/Holt59/bain-wizard-language-support"
    },
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "bainwizard",
                "aliases": [
                    "BAIN Wizard"
                ],
                "extensions": [
                    "wizard.txt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "bainwizard",
                "scopeName": "source.wizard",
                "path": "./syntaxes/wizard.tmLanguage.json"
            }
        ]
    }
}
